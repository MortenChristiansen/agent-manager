{
  "name": "agent-manager",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:dashboard\"",
    "dev:server": "bun --watch src/server/index.ts",
    "dev:dashboard": "vite",
    "build": "vite build && bun build src/server/index.ts --outdir dist/server --target bun",
    "start": "bun src/server/index.ts",
    "cli": "bun src/cli/index.ts"
  },
  "dependencies": {
    "ai": "^4.3.0",
    "@ai-sdk/openai": "^1.3.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.1.0",
    "postcss": "^8.5.0",
    "tailwindcss": "^4.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "typescript": "^5.7.0",
    "vite": "^6.1.0"
  },
  "bin": {
    "agent-manager": "./src/cli/index.ts"
  }
}
